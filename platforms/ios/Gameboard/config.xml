<?xml version='1.0' encoding='utf-8'?>
<widget id="co.gameboard.mobile.gb" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="FadeSplashScreen" value="true" />
    <preference name="FadeSplashScreenDuration" value=".25" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="ShowSplashScreenSpinner" value="true" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="TopActivityIndicator" value="gray" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="AdMob">
        <param name="ios-package" value="CDVAdMobPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="FacebookAds">
        <param name="ios-package" value="FacebookAdPlugin" />
    </feature>
    <feature name="FlurryAds">
        <param name="ios-package" value="FlurryAdPlugin" />
    </feature>
    <feature name="mMedia">
        <param name="ios-package" value="mMediaAdPlugin" />
    </feature>
    <feature name="MobFox">
        <param name="ios-package" value="MobFoxPlugin" />
    </feature>
    <feature name="StreamingMedia">
        <param name="ios-package" value="StreamingMedia" />
    </feature>
    <feature name="InAppBrowser">
        <param name="ios-package" value="CDVInAppBrowser" />
    </feature>
    <access origin="https://oauth.io" />
    <feature name="Console">
        <param name="ios-package" value="CDVLogger" />
    </feature>
    <feature name="Device">
        <param name="ios-package" value="CDVDevice" />
    </feature>
    <feature name="SplashScreen">
        <param name="ios-package" value="CDVSplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <name>Gameboard</name>
    <description>
        Add Videos and track the best.
    </description>
    <author email="info@game-board.co" href="http://game-board.co/">
      Game-board Ltd
    </author>
    <content src="index.html" />
    <access origin="https://oauth.io/*" />
    <access origin="https://google.com/*" />
    <access origin="*" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="false" />
    <preference name="UIWebViewBounce" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="AutoHideSplashScreen" value="false" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <plugins>
        <plugin name="InAppBrowser" value="org.apache.cordova.InAppBrowser" />
    </plugins>
</widget>
